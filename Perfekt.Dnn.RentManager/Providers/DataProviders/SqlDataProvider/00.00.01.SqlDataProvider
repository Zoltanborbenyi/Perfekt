ALTER TABLE {databaseOwner}{objectQualifier}RentManager_Items
ADD CONSTRAINT FK_RentManager_Products
FOREIGN KEY (ProductId)
REFERENCES {databaseOwner}{objectQualifier}RentManager_Products (ProductId)
ON DELETE CASCADE;
GO

